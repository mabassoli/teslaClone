---
import LandingHeader from "../components/LandingHeader.astro";
import HeroSection from "../components/HeroSection.astro";
import ModelS from "../components/ModelS.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="Tesla Landing">
	<LandingHeader />
	<main
		class="snap-y snap-mandatory relative w-full h-screen overflow-auto scroll-smooth"
	>
		<div class="snap-center">
			<HeroSection />
		</div>
		<div class="snap-center">
			<ModelS />
		</div>
	</main>
</Layout>
